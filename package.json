{
  "name": "dict",
  "version": "0.1.0",
  "private": true,
  "main": "main.ts",
  "description": "a dictionary app",
  "homepage": ".",
  "author": {
    "name": "bryht",
    "email": "bryht@live.com"
  },
  "build": {
    "appId": "Dict",
    "files": [
      "build/**/*",
      "main.ts"
    ],
    "extends": null,
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "bryht",
        "repo": "Dict"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.2.5",
    "fs-extra": "^9.0.0",
    "pdfkit": "^0.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "electron-builder": "^22.4.1",
    "electron": "^8.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
