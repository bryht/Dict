{
  "name": "dict",
  "version": "0.1.0",
  "private": true,
  "main": "main.ts",
  "description": "a dictionary app",
  "homepage": ".",
  "author": {
    "name": "bryht",
    "email": "bryht@live.com"
  },
  "build": {
    "appId": "Dict",
    "files": [
      "build/**/*",
      "main.ts"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "bryht",
        "repo": "dict"
      }
    ],
    "extends": null,
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/cheerio": "^0.22.17",
    "@types/electron-json-storage": "^4.0.0",
    "@types/jest": "^24.0.0",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "bootstrap": "^4.4.1",
    "cheerio": "^1.0.0-rc.3",
    "electron-is-dev": "^1.1.0",
    "electron-json-storage": "4.1.5",
    "electron-log": "^4.1.1",
    "electron-updater": "^4.2.5",
    "fs-extra": "^9.0.0",
    "guesslanguage": "^0.2.0",
    "jquery": "^3.4.1",
    "mousetrap": "^1.6.5",
    "node-google-translate-skidz": "^1.1.2",
    "node-sass": "^4.13.1",
    "pdfkit": "^0.11.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "redux-logger": "^3.0.6",
    "@types/redux-logger": "^3.0.7",
    "electron-builder": "^22.4.1",
    "electron-reload": "^1.5.0",
    "electron": "^8.2.0",
    "concurrently": "^5.1.0",
    "wait-on": "^4.0.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000  && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder --publish=never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
